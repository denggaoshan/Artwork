<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
   "http://struts.apache.org/dtds/struts-2.3.dtd">
    
<struts>
    <constant name="struts.devMode" value="true" />
    
    <constant name="struts.i18n.encoding" value="utf-8"></constant>
    
	<package name="struts2" namespace="/" extends="struts-default,json-default">
	 
	 	<action name="IndexAction" class="IndexAction">
	 		<result name="success">mains.jsp</result>
	 	</action>
		
		<action name="LoginAction" class="LoginAction" >
			<result type="json">   
				<param name="result">Result</param>  
            </result> 
		</action>

		<action name="RegisterAction" class="RegisterAction">
			<result type="json">   
				<param name="result">Result</param>  
            </result> 
		</action>
		
		<action name="AdvertorialAction" class="AdvertorialAction">
			<result name="success">advertorialDetail.jsp</result>
		</action>
		
	</package>
	
</struts>   
  

